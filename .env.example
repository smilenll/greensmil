# Contact email
NEXT_PUBLIC_CONTACT_EMAIL=your-email@example.com

# Social Links
NEXT_PUBLIC_GITHUB_URL=https://github.com/yourusername
NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/in/yourusername

# Google reCAPTCHA v3 (score-based, invisible)
# Get your keys from: https://www.google.com/recaptcha/admin
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Resend Email Configuration
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key

# Email addresses
# The verified sender email address in Resend (must be a domain you own)
RESEND_FROM_EMAIL=noreply@yourdomain.com

# Email address where contact form submissions will be sent (you receive notifications here)
RESEND_TO_EMAIL=your-email@example.com

# AWS Cognito Admin Service Credentials
# Create an IAM user with AmazonCognitoPowerUser policy attached
# Get credentials from: AWS Console → IAM → Users → Security credentials → Create access key
# Note: Cannot use AWS_ prefix on AWS Amplify (reserved), so using COGNITO_ prefix
COGNITO_ACCESS_KEY_ID=your_aws_access_key_id
COGNITO_SECRET_ACCESS_KEY=your_aws_secret_access_key
COGNITO_REGION=us-east-2

BUCKET_NAME=your-s3-bucket-name

AMPLIFY_APP_ID=your_amplify_app_id
AMPLIFY_BRANCH_NAME=main
AWS_REGION=us-east-2

# Google OAuth Configuration
# Get your credentials from: https://console.cloud.google.com/apis/credentials
# Create OAuth 2.0 Client ID with:
# - Authorized JavaScript origins: http://localhost:3000, https://yourdomain.com
# - Authorized redirect URIs:
#   http://localhost:3000/auth/callback
#   https://yourdomain.com/auth/callback
#   https://<your-cognito-domain>.auth.<region>.amazoncognito.com/oauth2/idpresponse
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# TinyMCE Editor
NEXT_PUBLIC_TEXT_EDITOR=your_tinymce_api_key